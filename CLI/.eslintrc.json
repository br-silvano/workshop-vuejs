{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:vue/essential",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "parser": [
      "babel-eslint"
    ]
  },
  "plugins": [
    "vue",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "array-callback-return": "off",
    "consistent-return": "off"
  }
}
